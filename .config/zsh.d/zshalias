# Rust Tools 

## exa
alias ls='exa --long --header --git --icons --group-directories-first'
alias la='exa --long --header --git --icons --group-directories-first -a'

## bat
alias cat='bat'

## Zoxide
alias cd='z'

# Useful Aliass
alias mkdir='mkdir -p'
alias youtube-dl='yt-dlp'

# Shotcuts
alias cl='clear'
alias clh='rm -rf $ZSH/zshistory'
alias vim='nvim'

# Scripts
alias screenshot='bash ~/.scripts/screenshot'
alias copy='bash ~/.scripts/copy'

# CleanUP
alias wget='wget --hsts-file="$XDG_DATA_HOME/wget-hsts"'
alias feh="feh --no-fehbg"

alias x='startx ~/.config/X11/xinitrc'
